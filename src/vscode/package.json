{
	"name": "datadog-to-yaml",
	"displayName": "Datadog to YAML",
	"description": "Converts Datadog monitor JSON into YAML",
	"version": "0.0.1",
	"publisher": "Koen Ekelschot",
	"repository": "https://github.com/peckham/Datadog-to-YAML",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:yaml"
	],
	"main": "../../dist/vscode/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pasteDatadogAsYAML",
				"title": "Paste Datadog as YAML"
			}
		],
		"keybindings": [
			{
				"command": "pasteDatadogAsYAML",
				"key": "ctrl+D ctrl+D",
				"mac": "cmd+D cmd+D",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "pasteDatadogAsYAML",
					"group": "9_cutcopypaste",
					"when": "resourceLangId == yaml"
				}
			]
		}
	}
}
