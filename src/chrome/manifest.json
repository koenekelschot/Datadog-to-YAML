{
  "manifest_version": 2,
  "name": "Datadog to YAML",
  "description": "Converts Datadog monitor JSON into YAML",
  "version": "0.1.1",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.datadoghq.eu/*"],
      "js": ["main.js", "vendor.js"]
    }
  ]
}