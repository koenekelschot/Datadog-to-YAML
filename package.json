{
	"scripts": {
		"vscode:prepublish": "webpack --config build/webpack.vscode.js --mode production",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch_tsc": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"watch-chrome": "webpack --config build/webpack.chrome.js --mode development --watch",
		"watch-vscode": "webpack --config build/webpack.vscode.js --mode development --watch",
		"build-chrome": "webpack --config build/webpack.chrome.js --mode development",
		"build-vscode": "webpack --config build/webpack.vscode.js --mode development",
		"build": "npm install && npm run build-chrome && npm run build-vscode"
	},
	"dependencies": {
		"js-yaml": "^3.5.5"
	},
	"devDependencies": {
		"@types/chrome": "0.0.100",
		"@types/glob": "^7.1.1",
		"@types/js-yaml": "^3.5.5",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"copy-webpack-plugin": "^5.1.1",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"ts-loader": "^6.2.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0",
		"webpack": "~4.42.0",
		"webpack-cli": "~3.3.11",
		"webpack-merge": "~4.2.2"
	}
}
